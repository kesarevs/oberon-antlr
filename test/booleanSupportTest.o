MODULE FigureList;
VAR
    true_a, true_b, false_c, false_d : BOOLEAN;
    intA, intB: INTEGER;
    realA, realB: REAL;
BEGIN
    true_a := TRUE;
    true_b := TRUE;
    false_c := FALSE;
    false_d := FALSE;

    true_a := true_a & true_b;
    false_c := false_c & false_d;
    false_c := false_c & true_a;

    true_a := true_a | true_a;
    true_a := true_a | true_b;
    true_a := true_a | false_c;
    false_c := false_c | false_d;

END FigureList.