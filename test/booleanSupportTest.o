MODULE FigureList;
VAR
    true_a, true_b, false_c, false_d : BOOLEAN;
    intA, intB: INTEGER;
    realA, realB: REAL;
BEGIN
    true_a := TRUE;
    true_b := TRUE;
    WRITELN(true_a = true_b);
    false_c := FALSE;
    false_d := FALSE;
    WRITELN(false_c = false_d);

    WRITELN(true_a & true_b = TRUE);
    WRITELN(false_c & false_d = FALSE);
    WRITELN(false_c & true_a = FALSE);

    WRITELN(true_a | true_a = TRUE);
    WRITELN(true_a | true_b = TRUE);
    WRITELN(true_a | false_c = TRUE);
    WRITELN(false_c | false_d = FALSE);

END FigureList.