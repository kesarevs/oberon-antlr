MODULE EPIFANOV_TEST;
VAR
	true_a, false_b : BOOLEAN;
    int1 : INTEGER;
BEGIN
    int1 := 2;
    true_a := TRUE;
	false_b := false_b | ~true_a;
    WRITELN(false_b = FALSE);
	false_b := false_b;
    WRITELN(false_b = FALSE);
	true_a := TRUE;
    WRITELN(true_a | false_b & false_b);
END EPIFANOV_TEST.
