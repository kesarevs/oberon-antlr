MODULE FigureList;
VAR
    MyINT : INTEGER;
    MyBool : BOOLEAN;
BEGIN
    IF 1 = 1 THEN
        MyBool := TRUE;
    END ;
    WRITELN(MyBool);

    IF 0 = 1 THEN
        MyBool := FALSE;
    END ;
    WRITELN(MyBool);

    IF 0 = 1 THEN
        MyBool := FALSE;
    ELSIF 0 = 1 THEN
        MyBool := FALSE;
    ELSIF 0 = 1 THEN
        MyBool := FALSE;
    ELSIF 1 = 1 THEN
        MyBool := TRUE;  
    END ;
    WRITELN(MyBool);

    IF 0 = 1 THEN
        MyBool := FALSE;
    ELSE
        MyBool := TRUE;  
    END ;
    WRITELN(MyBool);

    IF 0 = 1 THEN
        MyBool := FALSE;
    ELSIF 0 = 1 THEN
        MyBool := FALSE;
    ELSIF 0 = 1 THEN
        MyBool := FALSE;
    ELSE
        MyBool := TRUE;
    END ;
    WRITELN(MyBool);
END FigureList.