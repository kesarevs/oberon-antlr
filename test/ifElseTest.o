MODULE FigureList;
VAR
    MyINT : INTEGER;
BEGIN
    IF 1 = 1 THEN
        MyINT := 1;
    END ;

    IF 0 = 1 THEN
        MyINT := 0;
    END ;

    IF 0 = 1 THEN
        MyINT := 0;
    ELSIF 0 = 1 THEN
        MyINT := 0;
    ELSIF 0 = 1 THEN
        MyINT := 0;
    ELSIF 1 = 1 THEN
        MyINT := 1;   
    END ;

    IF 0 = 1 THEN
        MyINT := 0;
    ELSE
        MyINT := 1;   
    END ;

    IF 0 = 1 THEN
        MyINT := 0;
    ELSIF 0 = 1 THEN
        MyINT := 0;
    ELSIF 0 = 1 THEN
        MyINT := 0; 
    ELSE
        MyINT := 1;   
    END ;
END FigureList.