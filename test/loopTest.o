MODULE FigureList;
VAR
    MyINT : INTEGER;
BEGIN
    MyINT := 10;
    WHILE MyINT # 0 DO
        MyINT := MyINT - 1;
        WRITE(MyINT);
        WRITE(" ");
    END;
    WRITELN();
    MyINT := 10;
    REPEAT
        MyINT := MyINT - 1;
        WRITE(MyINT);
        WRITE(" ");
    UNTIL MyINT = 0;
END FigureList.