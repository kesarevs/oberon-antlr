MODULE FigureList;
VAR
    true_a, true_b, false_c, false_d : BOOLEAN;
    intA, intB, intC, intD: INTEGER;
    realA, realB, realC, realD: REAL;
BEGIN
    intA := 1;
    intB := -1;
    intC := 0;
    intD := 2;

    WRITELN(intA = intA);
    WRITELN(intB = intB);
    WRITELN(intA = -intB);
    false_c := intA = intB;

    WRITELN(false_c = FALSE);
    WRITELN(intA # intB);
    false_c := intA # -intB;
    WRITELN(false_c = FALSE);

    WRITELN(intA > intB);
    false_c := intA > intA;
    WRITELN(false_c = FALSE);
    false_c := intA > intD;
    WRITELN(false_c = FALSE);

    WRITELN(intB < intA);
    false_c := intA < intA;
    WRITELN(false_c = FALSE);
    false_c := intD < intA;
    WRITELN(false_c = FALSE);

    WRITELN(intA >= intB);
    WRITELN(intA >= intA);
    false_c := intA >= intD;
    WRITELN(false_c = FALSE);

    WRITELN(intB <= intA);
    WRITELN(intA <= intA);
    false_c := intD <= intA;
    WRITELN(false_c = FALSE);


END FigureList.