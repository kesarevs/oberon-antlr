MODULE FigureList;
VAR
    true, false : BOOLEAN;
    intA: INTEGER;
    realA: REAL;
BEGIN
    intA := 5;

    false := intA IN {1..4, 4..4, 4, 1};
    true := intA IN {1..5, 4..4, 4, 1};
    true := intA IN {1..4, 4..5, 4, 1};
    true := intA IN {1..4, 5..5, 4, 1};
    true := intA IN {1..4, 5..5, 5, 1};
    false := intA IN {};
    true := false IN {TRUE, FALSE};
    false := false IN {};

    realA := 5.5;
    true := realA IN {5.4, 5.5, 5.6};
    false := realA IN {5.4, 5.6};
    false := realA IN {5.4};
    false := realA IN {};

END FigureList.